{
  "name": "tweeter",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only ./src/index.ts",
    "dev:nodemon": "nodemon --exec ts-node --transpile-only ./src/index.ts",
    "run:script": "ts-node ./scripts/index.ts",
    "db:local": "docker-compose up -d",
    "generate": "npm run generate:prisma && npm run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node -r dotenv/config --transpile-only ./src/schema.ts",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:studio": "dotenv -- prisma studio --experimental --port 4001",
    "prisma:init": "npm run prisma:migrate:up"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@oakslab/ooo-config": "0.0.7",
    "@prisma/cli": "^2.13.0",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@nexus/schema": "^0.20.1",
    "@prisma/client": "^2.13.0",
    "apollo-server": "^2.19.0",
    "apollo-server-express": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "cuid": "^2.1.8",
    "dotenv-cli": "^4.0.0",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.27.0"
  }
}
